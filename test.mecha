\curry_add(lhs) {
    \(rhs) {
        return (lhs + rhs);
    }
};

let add = curry_add(1);
let result = add(1);

\recurse_deep(n, max_depth) {
    if (n <= max_depth) {
        print(n);
        return recurse_deep(n + 1, max_depth);
    }
};

recurse_deep(1, 10000);

while random() < 0.95 {
    print("hi")
};

\print_value_type(value) {
    print(type_of(value))
};

print_value_type(recurse_deep);
print_value_type(\() {});
print_value_type("hi " + " there");
print_value_type(1.0);
print_value_type([1, 1, 1]);

